<?xml version="1.0" ?>

<xs:schema xmlns="someNamespace or something.">

  <xs:element name="university"/>
    <xs:complexType>
      <xs:element name="faculty"/>
      <xs:attribute name="name" type="xs:string"/>
    </xs:complexType>
  </xs:element>

  <xs:element name="faculty"/>
    <xs:complexType>
      <xs:element name="department"/>
      <xs:attribute name="name" type="xs:string"/>
    </xs:complexType>
  </xs:element>

  <xs:element name="department"/>
    <xs:complexType>
       <xs:element name="group"/>
       <xs:attribute name="group_prefix" type="xs:string"/>
       <xs:attribute name="number" type="xs:positiveInteger"/>
       <xs:attribute name="name" type="xs:string"/>    
    </xs:complexType>
  </xs:element>

   <xs:element name="group"/>
    <xs:complexType>
      <xs:element name="student"/>
      <xs:attribute name="number" type="xs:positiveInteger"/>
      <xs:attribute name="entering" type="xs:positiveInteger"/>
    </xs:complexType>
  </xs:element> 


</xs:schema>
